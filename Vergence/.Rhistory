geom_point(aes(mean.V,meanFR),color='purple')+
stat_smooth(aes(mean.V,meanFR),color='black',se=FALSE,method=lm)+
xlab('')
nH<-ggplot(zp %>% filter(neuron==c1))+
geom_point(aes(mean.H,meanFR),color='orange')+
