abs(mean.R.H)<maxEP,
abs(mean.L.H)<maxEP,
abs(mean.R.V)<maxEP,
abs(mean.L.V)<maxEP,
meanFR<500)
tab<-zmc %>%
filter(neuron %in% c(c1,c2)) %>%
select(neuron,R2,H.R2,V.R2,dir.pref.slope,dir.pref.imp)
